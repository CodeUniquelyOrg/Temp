{
  "name": "Portal",
  "author": "Steve Saxton <steves@codeuniquely.co.uk>",
  "description": "Portal Application",
  "license": "UNLICENSED",
  "version": "0.1.0",
  "scripts": {
    "build:server": "babel ./app -d build",
    "build:watch:server": "babel ./app -d build --watch",
    "build:client": "webpack --config ./webpack.config.js/",
    "build:watch:client": "webpack --config ./webpack.config.js/ --watch",
    "build:prod": "npm run build:server && npm run build:client",
    "start": "npm run build:prod && NODE_ENV=production node ./build/server/server.js",
    "start:dev": "parallelshell 'npm run build:watch:client' 'npm run build:watch:server' 'nodemon ./build/server/index.js'",
    "serve:dev": "nodemon ./build/server/server.js",
    "start:dev:client": "webpack-dev-server",
    "lint": "eslint --ext .js src || true",
    "lint:fix": "eslint --ext .js src || true --fix",
    "test": "jest --watch  --coverage"
  },
  "dependencies": {
